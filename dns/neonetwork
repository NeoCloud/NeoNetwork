cklvax		IN	A	10.127.1.40
NNPCC		IN	A	10.127.1.63
pan		IN	A	10.127.3.1
staph		IN	A	10.127.8.185
staph-cn	IN	A	10.127.8.187
santost12	IN	A	10.127.69.1
tracker		IN	A	10.127.69.1
ipv4.tracker	IN	A	10.127.69.1
ipv6.tracker	IN	AAAA	fd10:127:ec39::1
kp		IN	A	10.127.69.1
kp		IN	AAAA	fd10:127:ec39::1

neko		IN	TXT	"helloworld"
telegram	IN	TXT	"https://t.me/NeoNetworkVPN"

; DELEGATED ZONES
jerry       IN  NS    ns1.jerry
